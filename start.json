{
  "run": [{
    "method": "shell.run",
    "params": {
      "path": "server",
      "venv": "env",
      "message": "uvicorn app:app",
      "on": [{
        "event": "/(http:\/\/[0-9.:]+)/i",
        "done": true
      }]
    }
  }, {
    "method": "self.set",
    "params": { "session.json": { "server": "{{input.event[1]}}" } }
  }, {
    "method": "shell.run",
    "params": {
      "path": "client",
      "message": "npm run dev",
      "on": [{
        "event": "/(http:\/\/[0-9.:]+)/i",
        "done": true
      }]
    }
  }, {
    "method": "self.set",
    "params": { "session.json": { "client": "{{input.event[1]}}" } }
  }, {
    "method": "browser.open",
    "params": {
      "uri": "{{self.session.client}}",
      "target": "_blank"
    }
  }]
}
